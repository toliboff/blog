<h1>Root users</h1>
<p>Here is a list of users</p>
<ul>
  <% @users.each do |user| %>
  <li class="users">
    <img src="<%= user.photo %>" class="users-image">
    <div class="users-info">
      <span><%= user.name %></span>
      <div>Number of posts: <%= user.posts_counter %></div>
    </div>
  </li>
  <% end %>
</ul>
